{
    "name": "marczhermo/solr-cloud",
    "description": "SolrCloud SilverStripe Module For Search",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "solr",
        "SolrCloud",
        "silverstripe",
        "search",
        "admin"
    ],
    "require": {
        "silverstripe/admin": "^1@dev",
        "silverstripe/framework": "^4@dev",
        "silverstripe/versioned": "^1@dev",
        "silverstripe/vendor-plugin": "^1.0",
        "marczhermo/search-list": "dev-master",
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "silverstripe/cms": "^4@dev"
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/lang"
        ]
    },
    "autoload": {
        "psr-4": {
            "Marcz\\Solr\\": "src/",
            "Marcz\\Solr\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "lint": "php phpcs.phar src/ tests/php/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
